<%= render 'admin/common/header' %>
<%= render 'admin/common/sidebar' %>
<!-- administrator main content start -->
<div id="content">
<div id="sub_content">
	
	<h1><%= t('menu_setting') %>&nbsp;-&nbsp;<%= t('title_show') %></h1>
</div>

<p>
  <strong><%= t('title_setting_name') %></strong>
  <%= @admin_setting.title %>
</p>

<p>
  <strong><%= t('title_setting_parameter') %></strong>
  <% if @admin_setting.columntype == 4 %>
  	<%= @select_graphsize.keys[@admin_setting.parameter.to_i] %>
  <% else %>
  	<%= @admin_setting.parameter %>
  <% end %>
</p>
<br /><br />
<%= link_to t('title_back'), admin_settings_path,:class=>"button_link" %>
<%= link_to t('title_edit'), edit_admin_setting_path(@admin_setting),:class=>"button_link" %>

</div><!-- administrator main content end -->
<%= render 'admin/common/footer' %>